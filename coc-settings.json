{
    "coc.preferences.extensionUpdateCheck": "daily",
    "languageserver": {
        "haxe": {
            "command": "node",
            "args": [
                "~/.vscode/extensions/nadako.vshaxe-2.23.1/server/bin/server.js"
            ],
            "filetypes": ["haxe"],
            "trace.server": "verbose",
            "initializationOptions": {
                "displayArguments": ["build.hxml"]
            },
            "settings": {
                "haxe.executable": "haxe"
            }
        }
    }
}
